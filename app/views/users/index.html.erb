<!--ユーザーの一覧情報-->

<%= render 'devise/shared/left', {user: @user, book: @book} %>


<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
</table>

  <% @user.each do |user| %>
  <div>
  <%= image_tag user.get_profile_image(100,100) %>
  <%= user.name %>
  <%= link_to "Show", user_path(current_user.id) %>
  </div>
  <% end %>
